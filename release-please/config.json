{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    "dotnet-tool": {
      "release-type": "simple",
      "extra-files": ["Directory.Build.props"],
      "changelog-sections": [
        {
          "type": "feat",
          "section": "🚀 Features",
          "hidden": false
        },
        {
